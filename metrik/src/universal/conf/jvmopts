-J-Xms6g
-J-Xmx6g
-J-XX:NewSize=4g
-J-XX:+UseConcMarkSweepGC
-J-XX:PermSize=256m
-J-verbose:gc
-J-XX:+PrintGCDetails
-J-XX:+PrintGCDateStamps
-J-Xloggc:logs/gc.log
-J-XX:+DisableExplicitGC
-J-XX:+TieredCompilation
-J-XX:ReservedCodeCacheSize=128m
-J-XX:InitialCodeCacheSize=128m
-J-XX:+HeapDumpOnOutOfMemoryError
